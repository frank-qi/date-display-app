node() {
    echo "Your Pipeline works!"
    sh('ls -la')
    
    sh('git clone "https://github.com/frank-qi/date-display-app"')
    
    echo "Here's what you cloned!"
    sh('ls -la')
    
    echo "Here's what you cloned!"
    
    echo "TESTING FOR PACKAGE MANAGERS"
    sh('apt-get --version')
    sh('yum --version')
    
    sh('npm install')
    sh('npm test')
}
